extends layout
block content
  h1.border.center= title
  h2.center Socket.IO Chat Room
  .chat.center
    p#num-users
    .chat-box
      ul#messages
      form(action='')
        input#m(autocomplete='off')
        button Send
    a#logout(href='/logout') Logout
  script(src='https://cdnjs.cloudflare.com/ajax/libs/jquery/3.5.1/jquery.min.js', integrity='sha256-9/aliU8dGd2tb6OSsuzixeV4y/faTqgFtohetphbbj0=', crossorigin='anonymous')
  <script src='/socket.io/socket.io.js'></script>
  script(async, defer, src='/public/javascripts/client.js')
